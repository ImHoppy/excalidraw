{
  "name": "excalidraw-server",
  "version": "1.0.0",
  "description": "Custom API server for Excalidraw backend",
  "main": "server.ts",
  "scripts": {
    "start": "tsx server.ts",
    "dev": "tsx --watch server.ts",
    "build": "tsc server.ts --outDir dist",
    "serve": "node dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.0",
    "@types/uuid": "^9.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.1.6"
  }
}
